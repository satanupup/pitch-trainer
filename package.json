  "author": "AI Pitch Trainer Team",
  "license": "ISC",
  "description": "AI-powered pitch training application with real-time vocal analysis and scoring system",
  "dependencies": {
    "@google-cloud/speech": "^7.1.0",
    "@google/generative-ai": "^0.24.1",
    "acrcloud": "^1.4.0",
    "axios": "^1.9.0",
    "cheerio": "^1.1.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
